					<string>function run( input, parameters ) {	// Unicode position of digits,	capital letters and regular letters:	const ᒻdigitsᒽ = "0".codePointAt ( 0 );	const ᒻcapsᒽ = "A".codePointAt ( 0 );	const ᒻlettersᒽ = "a".codePointAt ( 0 );	// To understand why we spread the characters below, refer to:	// https://dmitripavlutin.com/what-every-javascript-developer-should-know-about-unicode/	const digits = [ ..."⓪①②③④⑤⑥⑦⑧⑨" ];	const caps = [ ... "🄰🄱🄲🄳🄴🄵🄶🄷🄸🄹🄺🄻🄼🄽🄾🄿🅀🅁🅂🅃🅄🅅🅆🅇🅈🅉" ];	const letters = [ ... "🄰🄱🄲🄳🄴🄵🄶🄷🄸🄹🄺🄻🄼🄽🄾🄿🅀🅁🅂🅃🅄🅅🅆🅇🅈🅉" ];		// input is an object, it must be turned into a string	return [ ... input + "" ].		map ( c =&gt; { // replace digits, capitals and letters			// we do not rely on the assumption: 0 &lt; A &lt; a 			let d = c.codePointAt ( 0 );			d -= ᒻdigitsᒽ;			if ( d &gt;= 0 &amp;&amp; d &lt; 10 ) return digits [ d ];			d -= ᒻcapsᒽ - ᒻdigitsᒽ;			if ( d &gt;= 0 &amp;&amp; d &lt; 26 ) return caps [ d ];			d -= ᒻlettersᒽ - ᒻcapsᒽ;			if ( d &gt;= 0 &amp;&amp; d &lt; 26 ) return letters [ d ];			return c;			} ).		join ( "" );}// Test with: &lt;Here is an example of A Zozo with 0 ⚠️ and 1 😃&gt;</string>
